{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Content": {
      "type": "richtext"
    },
    "Excerpt": {
      "type": "string"
    },
    "Slug": {
      "type": "string",
      "unique": true
    },
    "CoverImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "blog"
    },
    "DatePublished": {
      "type": "datetime"
    }
  }
}
